# Allow password-less execution of kanata and pkill for kanata (Linux paths)
daphen ALL=(ALL) NOPASSWD: /usr/bin/kanata
daphen ALL=(ALL) NOPASSWD: /usr/bin/kanata *
daphen ALL=(ALL) NOPASSWD: /usr/bin/pkill kanata
daphen ALL=(ALL) NOPASSWD: /usr/bin/killall kanata
daphen ALL=(ALL) NOPASSWD: /usr/bin/nohup /usr/bin/kanata *
